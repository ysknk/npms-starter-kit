{
  "name": "npms-starter-kit",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "type": "module",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "ie >= 10",
    "Android >= 4"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run dev",
    "dev": "npm run watch",
    "generate": "npm run production:build --silent",
    "serv": "browser-sync start --config bs-config.cjs",
    "encode": "run-p encode:* --silent",
    "encode:templates": "iconv-lite -c \"src/templates/\" -s \"**/[!_]*.pug\" -e \".html\"",
    "encode:styles": "iconv-lite -c \"src/styles/\" -s \"**/[!_]*.styl\" -e \".css\"",
    "encode:scripts": "iconv-lite -c \"src/scripts/\" -s \"**/[!_]*.*\" -e \".js\"",
    "clean": "run-p clean:* --silent",
    "clean:templates": "del -c \"src/templates/\" -s \"**/[!_]*.pug\" -e \".html\"",
    "clean:styles": "del -c \"src/styles/\" -s \"**/[!_]*.styl\" -e \".css\"",
    "clean:scripts": "del -c \"src/scripts/\" -s \"**/[!_]*.*\" -e \".js\"",
    "clean:images": "del -c \"src/images/\" -s \"**/[!_]*.*\"",
    "clean:statics": "del -c \"src/statics/\" -s \"**/[!_]*.*\"",
    "build": "run-p build:* --silent",
    "build:templates": "pug -O .pugrc.js \"src/templates\" -o \"./dist/\" -E html",
    "build:styles": "stylus",
    "build:scripts": "webpack",
    "build:images": "imagemin",
    "build:statics": "cpx \"src/statics/**/[!_]*\" \"./dist/\" -v",
    "watch": "run-p serv watch:*  --silent",
    "watch:templates": "npm run build:templates -- -w",
    "watch:styles": "chokidar \"src/styles/**/*\" -c \"if [[ '{event}' =~ add||change ]]; then npm run build:styles; fi;\"",
    "watch:scripts": "chokidar \"src/scripts/**/*\" -c \"if [[ '{event}' =~ add||change ]]; then npm run build:scripts; fi;\"",
    "watch:images": "chokidar \"src/images/**/*\" -c \"if [[ '{event}' =~ add||change ]]; then npm run build:images -- -s {path}; fi;\"",
    "watch:statics": "npm run build:statics -- -w --no-initial",
    "lint": "run-s lint:* --silent",
    "lint:templates": "htmlhint --config .htmlhintrc.js \"./dist/**/*.html\"",
    "lint:styles": "stylelint --config .stylelintrc.cjs \"./dist/**/*.css\" -f verbose --aei --color",
    "lint:scripts": "eslint -c .eslintrc.cjs \"./src/scripts/**/*\"",
    "production:build": "cross-env NODE_ENV=production npm run build --silent && npm run lint --silent",
    "development:build": "cross-env NODE_ENV=development npm run build --silent",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "init-storybook": "npx storybook init --type html"
  },
  "keywords": [
    "npm-scripts"
  ],
  "author": "ysknk",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-controls": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack4": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/html": "^6.5.15",
    "@storybook/manager-webpack4": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "ajv": "^8.12.0",
    "babel-loader": "^9.1.0",
    "browser-sync": "^2.27.10",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "core-js": "^3.26.1",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.8",
    "glob": "^8.0.3",
    "glob-del": "github:ysknk/glob-del",
    "glob-iconv-lite": "github:ysknk/glob-iconv-lite",
    "glob-imagemin": "github:ysknk/glob-imagemin",
    "glob-stylus": "github:ysknk/glob-stylus",
    "htmlhint": "^1.1.4",
    "node-package-utilities": "github:ysknk/node-package-utilities",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "pug": "^3.0.2",
    "pug-cli": "github:pugjs/pug-cli#master",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylus-loader": "^7.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "volta": {
    "node": "18.12.1",
    "npm": "8.19.3"
  }
}
